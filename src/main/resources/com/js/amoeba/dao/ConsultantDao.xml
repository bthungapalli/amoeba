<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.js.amoeba.dao.ConsultantDao">

	<select id="getAllConsultant" resultType="com.js.amoeba.domain.Consultant"
		>
		
	</select>
	
	<insert id="saveConsultant" parameterType="com.js.amoeba.domain.Consultant">
		INSERT INTO
		consultant(experience,
		specialization,avalibality,simultaneous,uId,subCat)
		VALUES
		(#{experience},#{specialization},#{avalibality},#{simultaneous},#{uId},#{subCat})
	</insert>
	 
	</mapper>
	