<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.js.amoeba.dao.UserFormDao">

	
	<insert id="saveForm" parameterType="com.js.amoeba.domain.UserForm">
		INSERT INTO
		userform(weight,height,age,specialization,majorCat,subCat,status,c_id,uId)
		VALUES
		(#{weight},#{height},#{age},#{specialization},#{majorCat},#{subCat},#{status},#{c_id},#{uId})
	</insert>
	 
	</mapper>
	